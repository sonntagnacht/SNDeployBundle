{
  "name"        : "sonntagnacht/deploy-bundle",
  "description" : "command to deploy your project for symfony 2.8+",
  "type"        : "symfony-bundle",
  "authors"     : [
    {
      "name"  : "Team Internet",
      "email" : "dev@sonntagnacht.de"
    }
  ],
  "license"     : [
    "MIT"
  ],
  "require"     : {
    "php"                         : ">=5.6",
    "sonntagnacht/toolbox-bundle" : "^0.2",
    "vierbergenlars/php-semver"   : "3.0.*"
  },
  "config"      : {
    "preferred-install" : "dist",
    "platform"          : {
      "php" : "5.5.9"
    }
  },
  "require-dev" : {
    "phpunit/phpunit"                    : "3.7.*",
    "symfony/framework-standard-edition" : "3.*",
    "symfony/security-acl"               : "3.*",
    "symfony/proxy-manager-bridge"       : "3.2.*"
  },
  "autoload"    : {
    "psr-4" : {
      "SN\\DeployBundle\\" : ""
    }
  }
}
